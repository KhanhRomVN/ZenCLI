{
  "name": "zencli",
  "type": "module",
  "version": "1.0.0",
  "description": "A CLI tool for interacting with Claude AI",
  "main": "lib/bin/run.js",
  "bin": {
    "zencli": "./bin/run.js"
  },
  "oclif": {
    "bin": "zencli",
    "commands": "./lib/cli/commands"
  },
  "files": [
    "bin",
    "lib",
    "npm-shrinkwrap.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run copy-electron",
    "copy-electron": "mkdir -p lib/core/lib/electron && cp src/core/lib/electron/electron-app.cjs lib/core/lib/electron/electron-app.cjs",
    "dev": "tsc -w -p tsconfig.json",
    "postinstall": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write 'src/**/*.ts'",
    "link": "npm link"
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@types/react": "^19.2.7",
    "chalk": "^4.1.2",
    "cli-select": "^1.1.2",
    "conf": "^10.2.0",
    "cors": "^2.8.5",
    "electron": "^39.2.6",
    "enquirer": "^2.4.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "figlet": "^1.6.0",
    "form-data": "^4.0.5",
    "gradient-string": "^2.0.2",
    "helmet": "^8.1.0",
    "ink": "^6.5.1",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^8.2.7",
    "node-fetch": "^2.7.0",
    "open": "^9.1.0",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "puppeteer-core": "^22.7.1",
    "react": "^19.2.3",
    "uuid": "^13.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.19",
    "@types/electron": "^1.4.38",
    "@types/express": "^5.0.6",
    "@types/figlet": "^1.5.8",
    "@types/form-data": "^2.2.1",
    "@types/gradient-string": "^1.1.3",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.11",
    "@types/ora": "^3.2.0",
    "@types/prompts": "^2.4.9",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.54.0",
    "oclif": "^4.22.54",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "claude",
    "ai",
    "cli",
    "chat",
    "anthropic",
    "zencli"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/zencli"
  }
}
