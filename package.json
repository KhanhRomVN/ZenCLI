    "js-yaml": "^4.1.1"
  },
  "c8": {
    "reporter": [
      "lcov",
      "html"
    ],
    "exclude": [
      "**/testing-platform/**",
      "**/.vscode-test/**",
      "**/node_modules/**",
      "node_modules",
      "**/dist-standalone/src/**",
      "**/dist-standalone/vsce-extension/https:/**",
      "**/dist-standalone/vsce-extension/**",
      "**/dist-standalone/https:/**",
      "**/dist-standalone/LIB/src/**",
      "**/dist-standalone/pdfjs-dist/**",
      "**/*.d.ts",
      "**/*.{test,spec}.{js,jsx,ts,tsx,mjs,cjs}",
      "**/__tests__/**",
      "**/test/**",
      "**/tests/**",
      "**/.nyc_output/**",
      "**/tests-results/**",
      "src/test/**",
      "**/src/xml/**",
      "**/standalone/**",
      "**/src/generated/**",
      "**/evals/cli/dist/**",
      "**/evals/cli/src/**",
      "dist"
    ],
    "all": true,
    "exclude-after-remap": true
  }
}
